<h1 id="title" align="center">Create Item</h1>
<%= form_for @item do |f|%>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :description %>
<%= f.text_area :description %><br>
<%= f.label :image %>
<%= f.file_field :image %>
<%= f.submit %>
<%end%>
<% if flash[:error] %>
  <p>
   <%= flash[:error] %>
  </p>
<% end %>